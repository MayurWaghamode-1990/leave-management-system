# Production Environment Variables
# Leave Management System - Frontend

# API Configuration
VITE_API_URL=http://localhost:3002/api/v1

# Application Configuration
VITE_APP_NAME=Leave Management System
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Enterprise Leave Management System

# Environment
NODE_ENV=production
VITE_ENV=production

# Performance Monitoring
VITE_ENABLE_PERFORMANCE_MONITOR=false
VITE_ENABLE_ANALYTICS=true

# Feature Flags
VITE_FEATURE_COMP_OFF=true
VITE_FEATURE_MULTI_LEVEL_APPROVALS=true
VITE_FEATURE_ENHANCED_ANALYTICS=true
VITE_FEATURE_REAL_TIME_NOTIFICATIONS=true

# Build Configuration
GENERATE_SOURCEMAP=false
VITE_BUILD_ANALYZE=false

# Security
VITE_SECURE_COOKIES=true
VITE_SESSION_TIMEOUT=3600000

# Logging
VITE_LOG_LEVEL=error
VITE_ENABLE_CONSOLE_LOGS=false